{
  "routes": [
    {
      "route": "/*.{css,js,png,jpg,jpeg,gif,svg,ico,woff,woff2,ttf,eot}",
      "headers": {
        "cache-control": "public, max-age=0, must-revalidate"
      }
    },
    {
      "route": "/api/*",
      "allowedRoles": ["anonymous"]
    },
    {
      "route": "/*",
      "serve": "/index.html",
      "statusCode": 200
    }
  ],
  "mimeTypes": {
    ".js": "application/javascript",
    ".css": "text/css",
    ".json": "application/json",
    ".png": "image/png",
    ".jpg": "image/jpeg",
    ".svg": "image/svg+xml"
  }
} 